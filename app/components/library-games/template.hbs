<div class="row">
  <div class="col-xs-6">
    <h3>Backlog</h3>
    {{#each library as |lib|}}
      {{#if lib.done}}
        {{library-thumbnail lib=lib}}
        <p>{{library.done}}</p>
      {{/if}}
    {{/each}}
  </div>

  <div class="col-xs-6">
    <h3>Completed</h3>
    {{#each library as |lib|}}
      {{#if notDone}}
        {{library-thumbnail lib=lib}}
        <p>{{library.done}}</p>
      {{/if}}
    {{/each}}
  </div>
</div>
